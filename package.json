{
  "name": "simple-ot",
  "version": "1.0.0",
  "description": "Operational transform using the GOTO control algorithm",
  "author": "Russell Clarey (https://russellclarey.com)",
  "keywords": ["operational transform", "OT"],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["lib/**/*"],
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverage": false,
    "coverageDirectory": "<rootDir>/coverage/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "test/.*\\.test\\.ts",
    "moduleFileExtensions": ["ts", "js"],
    "modulePaths": ["<rootDir>/src/"]
  }
}
