{
  "compilerOptions": {
    "declaration": true,
    "module": "esnext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "removeComments": true,
    "strict": true,
    "target": "ES6",
    "allowJs": false,
    "checkJs": false,
    "esModuleInterop": true,
    "jsx": "react",
    "plugins": [
      {
        "name": "typescript-deno-plugin"
      }
    ]
  },
  "include": ["./dist/*.ts"],
  "exclude": []
}
